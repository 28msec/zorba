<?xml version="1.0" encoding="UTF-8"?>

<zorba:iterators
  xmlns:zorba="http://www.zorba-xquery.com"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.zorba-xquery.com ../runtime.xsd">
    
<!--
/*******************************************************************************
* 16.3 fn:current-dateTime
********************************************************************************/
-->
<zorba:iterator name="CurrentDateTimeIterator">

    <zorba:description author="Zorba Team">fn:current-dateTime</zorba:description>
    
    <zorba:function>

        <zorba:signature localname="current-dateTime" prefix="fn">
            <zorba:output>xs:dateTime</zorba:output>
        </zorba:signature>

      <zorba:methods>
        <zorba:accessesDynCtx returnValue="true"/>
      </zorba:methods>

    </zorba:function>
    
</zorba:iterator>

