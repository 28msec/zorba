<?xml version="1.0" encoding="UTF-8"?>

<zorba:iterators
  xmlns:zorba="http://www.zorba-xquery.com"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.zorba-xquery.com ../runtime.xsd">

<!--
/*******************************************************************************
  fn:put
********************************************************************************/
-->
<zorba:iterator name="FnPutIterator">
    
    <zorba:description author="Zorba Team">fn:put</zorba:description>
  
  <zorba:function>

    <zorba:signature localname="put" prefix="fn">
      <zorba:param>node()</zorba:param>
      <zorba:param>xs:string</zorba:param>
      <zorba:output>empty-sequence()</zorba:output>
    </zorba:signature>

    <zorba:methods>
      <zorba:getScriptingKind returnValue="UPDATING_EXPR"/>
      <zorba:accessesDynCtx returnValue="true"/>      
    </zorba:methods>

  </zorba:function>

</zorba:iterator>


</zorba:iterators>
