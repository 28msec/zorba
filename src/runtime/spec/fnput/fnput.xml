<?xml version="1.0" encoding="UTF-8"?>

<zorba:iterators xmlns:zorba="http://www.zorba-xquery.com">

<!--
/*******************************************************************************
  fn:put
********************************************************************************/
-->
<zorba:iterator name="FnPutIterator">
    
    <zorba:description author="Zorba Team">fn:put</zorba:description>
  
  <zorba:function requiresDynamicContext="false"
                  isSource="false"
                  isUpdating="true"
                  propagatesInputToOutput="false">
    <zorba:signature localname="put" prefix="fn">
      <zorba:param>node()</zorba:param>
      <zorba:param>xs:string</zorba:param>
      <zorba:output>empty-sequence()</zorba:output>
    </zorba:signature>
  </zorba:function>
</zorba:iterator>


</zorba:iterators>
