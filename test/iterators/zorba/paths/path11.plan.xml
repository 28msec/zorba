<iterator-tree description="main query">
  <NodeSortIterator distinct="true" ascending="true">
    <ChildAxisIterator test-kind="match_name_test" qname="xs:QName(,,b)" typename="*" nill-allowed="false">
      <FLWORIterator>
        <ForVariable name="x">
          <ElementIterator>
            <SingletonIterator value="xs:QName(,,a)"/>
            <FnConcatIterator>
              <ElementIterator>
                <SingletonIterator value="xs:QName(,,b)"/>
                <AttributeIterator qname="xs:QName(,,id)">
                  <SingletonIterator value="xs:string(1)"/>
                </AttributeIterator>
              </ElementIterator>
              <ElementIterator>
                <SingletonIterator value="xs:QName(,,b)"/>
                <AttributeIterator qname="xs:QName(,,id)">
                  <SingletonIterator value="xs:string(2)"/>
                </AttributeIterator>
              </ElementIterator>
            </FnConcatIterator>
          </ElementIterator>
        </ForVariable>
        <ForVariable name="y">
          <FnConcatIterator>
            <SingletonIterator value="xs:integer(1)"/>
            <SingletonIterator value="xs:integer(2)"/>
            <SingletonIterator value="xs:integer(3)"/>
          </FnConcatIterator>
        </ForVariable>
        <ReturnClause>
          <ForVarIterator varname="x"/>
        </ReturnClause>
      </FLWORIterator>
    </ChildAxisIterator>
  </NodeSortIterator>
</iterator-tree>
