import sys
sys.path.append('@pythonPath@')
import zorba_api

lAPI = zorba_api.new_apip()
zorba_api.zorba_init(lAPI)

lErr = zorba_api.new_errorp()
lItem = zorba_api.item_init(lAPI)
lString = zorba_api.string_init(lAPI)

lXQuery = zorba_api.query_compile(lAPI, 'for $x in (1+2,3+4, "this is a test", 9) return $x', lErr)
lResult = zorba_api.sequence_init(lXQuery, lErr)

while 0 < zorba_api.sequence_next(lResult, lItem, lErr):
  zorba_api.item_stringvalue(lItem, lString, lErr)
  print zorba_api.string_to_char(lString)

zorba_api.sequence_close(lResult)
zorba_api.query_release(lXQuery)

zorba_api.string_release(lString)
zorba_api.item_release(lItem)
zorba_api.delete_errorp(lErr)
zorba_api.zorba_release()
zorba_api.delete_apip(lAPI)
